{
    "manifest_version": 2,

    "name": "E.mo",
    "version": "0.1",
    "description": "Select & Paste emoji utility",
    "browser_action": {
        "default_icon": "img/Icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "img/Icon.png",
        "48": "img/Icon.png",
        "128": "img/Icon.png"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["js/script.js"]
    }],
    "permissions": [
        "activeTab"
    ]
}
